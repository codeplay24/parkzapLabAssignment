<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
    <style media="screen">
      body{
        text-align: center;
      }
      #table{
        margin-left:auto;
        margin-right:auto;
        width: 700px;
        margin-top: 100px;

      }
    </style>
  </head>
  <body>
    <h1>ParkzapLabs Assignment Data Table</h1>
    <table border="1" cellspacing="0" id="table">
      <tr>
        <th>Name</th>
        <th>email</th>
        <th>Phone Number</th>
        <th>DoB</th>
      </tr>
      {{#each userArr}}
        <tr>
          <td>{{this.name}}</td>
          <td>{{this.email}}</td>
          <td>{{this.phoneNumber}}</td>
          <td>{{this.dob}}</td>
        </tr>
      {{/each}}
    </table>
  </body>
</html>
